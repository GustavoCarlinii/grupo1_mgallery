    <%- include("partials/head", {styles: ['style-header-footer', 'ingreso', 'style-carrito'], title: 'Registro'}) %>
    <body>
        <%- include("partials/header") %>
        <main class="main-carrito">
            <form action="">
                <h1>Crea tu cuenta</h1>
                <div class="container">
                    <div class="form-container">
                        <label for="nombre">Nombre</label>
                        <input type="text" name="nombre-reg" id="nombre">
                    </div>
                    <div class="form-container">
                        <label for="apellido">Apellido</label>
                        <input type="text" name="apellido-reg" id="apellido">
                    </div>
                    <div class="form-container">
                        <label for="email">Correo electronico</label>
                        <input type="email" name="email-reg" id="email">
                    </div>
                    <div class="form-container">
                        <label for="password">Contraseña</label>
                        <input type="password" name="password-reg" id="password">
                    </div>
                    <div class="form-container">
                        <label for="password">Repetir contraseña</label>
                        <input type="password" name="password-reg" id="password">
                    </div>
                </div>
                <div class="boton-carrito">
                    <button type="reset">Borrar</button>
                    <button type="submit">Crear</button>
                </div>
            </form>
        </main>
        <%- include("partials/footer") %>
</body>
</html>